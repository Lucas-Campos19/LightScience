@model IEnumerable<LightScience.Models.Lux>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuantidadeLux)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataLeitura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cutura)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuantidadeLux)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataLeitura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cutura.Categoria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LuxId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LuxId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LuxId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
